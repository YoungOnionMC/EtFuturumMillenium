{
  "multipart": [
    {   "when": { "age": "0" },
      "apply": [
        { "model": "etfuturum:bamboo/bamboo1_age0" },
        { "model": "etfuturum:bamboo/bamboo2_age0" },
        { "model": "etfuturum:bamboo/bamboo3_age0" },
        { "model": "etfuturum:bamboo/bamboo4_age0" }
      ]
    },
    {   "when": { "age": "1" },
      "apply": [
        { "model": "etfuturum:bamboo/bamboo1_age1" },
        { "model": "etfuturum:bamboo/bamboo2_age1" },
        { "model": "etfuturum:bamboo/bamboo3_age1" },
        { "model": "etfuturum:bamboo/bamboo4_age1" }
      ]
    },
    {   "when": { "leaves": "small" },
      "apply": { "model": "etfuturum:bamboo/bamboo_small_leaves" }
    },
    {   "when": { "leaves": "large" },
      "apply": { "model": "etfuturum:bamboo/bamboo_large_leaves" }
    }
  ]
}
